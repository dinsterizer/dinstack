    "@trpc/react-query": "^10.44.1",
    "framer-motion": "^10.16.16",
    "jotai": "^2.6.0",
    "next": "14.0.3",
    "next-themes": "^0.2.1",
    "oslo": "^0.23.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
