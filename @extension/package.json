{
  "name": "@dinstack/extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "env-cmd -f ../@web/.env.local vite",
    "build": "tsc && env-cmd -f ../@web/.env.local vite build",
    "build:preview": "tsc && env-cmd -f ../@web/.env.preview vite build --mode=preview",
    "build:production": "tsc && env-cmd -f ../@web/.env.production vite build --mode=production",
    "typecheck": "tsc --noEmit",
    "preview": "env-cmd -f ../@web/.env.local vite build"
  },
  "dependencies": {
    "@dinstack/api": "workspace:^",
    "@dinstack/web": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "superjson": "^2.2.1",
    "ts-pattern": "^5.0.6",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.21",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/chrome": "^0.0.256",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite": "^4.5.1"
  }
}
