{
  "extends": "@dinstack/tsconfig/react.json",
  "compilerOptions": {
    "outDir": "build",
    "paths": {
      "@web/*": ["./*"],
      "@ui/*": ["../../packages/ui/*"],
      "@ui/_/../*": ["../../packages/ui/*"],
      "@shared-react/*": ["../../packages/shared-react/*"],
      "@turnstile-react/*": ["../../packages/turnstile-react/*"],
      "@auth-react/*": ["../../packages/auth-react/*"]
    }
  },
  "exclude": ["vite.config.ts", "build", "dist", "tailwind.config.ts"],
  "references": [
    {
      "path": "../../packages/ui/tsconfig.json"
    },
    {
      "path": "../../packages/shared-react/tsconfig.json"
    },
    {
      "path": "../../packages/turnstile-react/tsconfig.json"
    },
    {
      "path": "../../packages/auth-react/tsconfig.json"
    }
  ]
}
