{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "dev:web": "pnpm run dev",
    "build": "tsc --build && tsc-alias",
    "preview": "vite build && vite preview --port 4000",
    "typecheck": "tsc --noEmit",
    "deploy:preview": "vite build --mode=preview && wrangler pages deploy --project-name=dinstack-web-preview --branch=main ./dist",
    "deploy:production": "vite build --mode=production && wrangler pages deploy --project-name=dinstack-web-production --branch=main ./dist"
  },
  "dependencies": {
    "@dinstack/api": "workspace:^",
    "@dinstack/ui": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@dinstack/tsconfig": "workspace:^",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.8"
  }
}
