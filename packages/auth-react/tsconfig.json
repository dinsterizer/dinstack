{
  "extends": "@dinstack/tsconfig/react.json",
  "compilerOptions": {
    "outDir": "build",
    "paths": {
      "@auth-react/*": ["./*"],
      "@shared/*": ["../shared/*"],
      "@shared-react/*": ["../shared-react/*"],
      "@db/*": ["../db/*"],
      "@ui/*": ["../ui/*"],
      "@ui/_/../*": ["../ui/*"]
    }
  },
  "exclude": ["build"],
  "references": [
    {
      "path": "../shared/tsconfig.json"
    },
    {
      "path": "../shared-react/tsconfig.json"
    },
    {
      "path": "../db/tsconfig.json"
    },
    {
      "path": "../ui/tsconfig.json"
    }
  ]
}
