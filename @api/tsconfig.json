{
  "extends": "@dinstack/tsconfig/worker.json",
  "compilerOptions": {
    "outDir": "build",
    "paths": {
      "@api/*": ["./*"],
      "+shared/*": ["../+shared/*"],
      "+db/*": ["../+db/*"]
    }
  },
  "exclude": ["build"],
  "references": [
    {
      "path": "../+shared/tsconfig.json"
    },
    {
      "path": "../+db/tsconfig.json"
    }
  ]
}
